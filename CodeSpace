cat > apps/tables/apps.py << 'EOF'
from django.apps import AppConfig

class TablesConfig(AppConfig):
    default_auto_field = 'django.db.models.BigAutoField'
    name = 'apps.tables'
    verbose_name = 'Table Management'
EOF

